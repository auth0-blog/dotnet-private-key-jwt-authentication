{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "Authority": "https://<YOUR-AUTH0-DOMAIN>",
        "ValidAudiences": [ "<YOUR-API-IDENTIFIER>" ],
        "ValidIssuer": "<YOUR-AUTH0-DOMAIN>"
      }
    }
  }
}